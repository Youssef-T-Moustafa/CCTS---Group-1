<h1>Students List</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Student Id</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>CCTS Score</th>
      <th>Student Information</th>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td data-label="Student Id"><%= student.id %></td>
        <td data-label="First Name"><%= student.first_name %></td>
        <td data-label="Last Name"><%= student.last_name %></td>
        <td data-label="Email"><%= student.parent_email %></td>
        <td data-label="CCTS Score"><%= student.score %></td>
        <td>
          <%= link_to 'View Information', stuInfo_staff_path(id: student.id), class: 'btn btn-primary' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Back to Dashboard', dashboard_path, class: 'btn btn-primary mb-3 col-12' %>
