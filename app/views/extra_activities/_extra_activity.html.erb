<h2>External Activity Details</h2>
<table class="table table-striped table-bordered">
  <tbody>
    <tr>
      <th >Name</th>
      <td><%= extra_activity.name %></td>
    </tr>
    <tr>
      <th >Description</th>
      <td><%= extra_activity.description %></td>
    </tr>
    <tr>
      <th >Start Date</th>
      <td><%= extra_activity.start_date %></td>
    </tr>
    <tr>
      <th >End Date</th>
      <td><%= extra_activity.end_date %></td>
    </tr>
  </tbody>
</table>
