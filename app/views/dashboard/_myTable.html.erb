<h1>Staffs</h1>

<table class="table table-striped">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Gender</th>
    <th>Email</th>
    <th>Phone</th>
  </tr>
  <% @staffs.each do |staff| %>
  <tr>
    <td><%= staff.first_name %></td>
    <td><%= staff.last_name %></td>
    <td><%= staff.gender %></td>
    <td><%= staff.email %></td>
    <td><%= staff.phone %></td>
  </tr>
  <% end %>
</table>
